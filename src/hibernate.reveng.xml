<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="app_locationshare" match-name="tb_exception"/>
  <table-filter match-catalog="app_locationshare" match-name="tb_logininfo"/>
  <table-filter match-catalog="app_locationshare" match-name="tb_user"/>
  <table-filter match-catalog="app_locationshare" match-name="tb_user_detail"/>
</hibernate-reverse-engineering>